{
    "name": "myorg",
    "private": true,
    "scripts": {
        "dev": "moon :dev",
        "build": "moon :build",
        "prepare": "moon setup",
        "lint": "biome lint . --write",
        "check": "biome check . --write",
        "format": "biome format . --write",
        "cleanup": "pnpm run --color \"/cleanup:/\"",
        "cleanup:dev": "pnpm -r --parallel --stream run cleanup",
        "cleanup:cache": "pnpm store prune --force && pnpm dlx rimraf .moon/cache",
        "cleanup:deps": "pnpm dlx rimraf pnpm-lock.yaml node_modules",
        "update-deps": "npm-check-updates --configFileName .ncurc.json"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "npm-check-updates": "^17.1.14"
    },
    "packageManager": "pnpm@10.2.0",
    "pnpm": {
        "ignoredBuiltDependencies": [
            "esbuild"
        ],
        "onlyBuiltDependencies": [
            "sharp"
        ]
    }
}
