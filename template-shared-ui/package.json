{
    "name": "@repo/shared-ui",
    "private": true,
    "type": "module",
    "files": ["dist"],
    "exports": {
        "./components": {
            "types": "./dist/components.d.ts",
            "default": "./dist/components/index.js"
        },
        "./hooks": {
            "types": "./dist/hooks.d.ts",
            "default": "./dist/hooks/index.js"
        },
        "./theme": {
            "types": "./dist/theme.d.ts",
            "default": "./dist/theme/index.js"
        },
        "./utils": {
            "types": "./dist/utils.d.ts",
            "default": "./dist/utils/index.js"
        }
    },
    "typesVersions": {
        "*": {
            "components": ["./dist/components.d.ts"],
            "hooks": ["./dist/hooks.d.ts"],
            "theme": ["./dist/theme.d.ts"],
            "utils": ["./dist/utils.d.ts"]
        }
    },
    "scripts": {
        "dev": "vite",
        "build:vite": "vite build",
        "build": "pnpm run --color \"/build:vite/\"",
        "storybook": "VITE_CJS_IGNORE_WARNING=true storybook dev -p 6006 --no-open",
        "build:storybook": "VITE_CJS_IGNORE_WARNING=true storybook build -o storybook-static",
        "cleanup": "pnpm dlx rimraf dist node_modules pnpm-lock.yaml storybook-static",
        "start": "pnpm dlx serve storybook-static -l 6006 -n -C",
        "prepublishOnly": "pnpm run build:vite",
        "typecheck": "tsc --noEmit"
    },
